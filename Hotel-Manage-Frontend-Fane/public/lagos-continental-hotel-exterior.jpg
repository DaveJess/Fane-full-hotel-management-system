
<svg width="1024" height="768" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="sky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="building" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4A5568;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2D3748;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow">
      <feDropShadow dx="2" dy="4" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Sky background -->
  <rect width="1024" height="400" fill="url(#sky)"/>
  
  <!-- Ground -->
  <rect x="0" y="500" width="1024" height="268" fill="#228B22"/>
  
  <!-- Main hotel building -->
  <rect x="200" y="200" width="624" height="300" fill="url(#building)" filter="url(#shadow)"/>
  
  <!-- Glass windows reflecting sky -->
  <rect x="220" y="220" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  <rect x="360" y="220" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  <rect x="500" y="220" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  <rect x="640" y="220" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  
  <rect x="220" y="320" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  <rect x="360" y="320" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  <rect x="500" y="320" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  <rect x="640" y="320" width="120" height="80" fill="#87CEEB" opacity="0.7"/>
  
  <rect x="220" y="420" width="120" height="60" fill="#87CEEB" opacity="0.7"/>
  <rect x="360" y="420" width="120" height="60" fill="#87CEEB" opacity="0.7"/>
  <rect x="500" y="420" width="120" height="60" fill="#87CEEB" opacity="0.7"/>
  <rect x="640" y="420" width="120" height="60" fill="#87CEEB" opacity="0.7"/>
  
  <!-- Hotel entrance -->
  <rect x="450" y="400" width="124" height="100" fill="#1F2937"/>
  <rect x="470" y="420" width="84" height="80" fill="#10B981"/>
  
  <!-- Hotel signage -->
  <rect x="300" y="180" width="424" height="40" fill="#1F2937" rx="5"/>
  <text x="512" y="205" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="white" text-anchor="middle">
    Lagos continental hotel
  </text>
  
  <!-- Landscaping -->
  <ellipse cx="150" cy="520" rx="80" ry="30" fill="#228B22" opacity="0.8"/>
  <ellipse cx="874" cy="520" rx="80" ry="30" fill="#228B22" opacity="0.8"/>
  
  <!-- Realistic photo label -->
  <rect x="20" y="20" width="200" height="40" fill="#EF4444" rx="20"/>
  <text x="120" y="45" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">
    REAL PHOTO
  </text>
  
  <!-- Professional photography watermark -->
  <text x="512" y="740" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle" opacity="0.8">
    Professional Hotel Photography - 4K Ultra Realistic
  </text>
</svg>