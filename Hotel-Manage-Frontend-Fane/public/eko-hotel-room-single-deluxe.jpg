
<svg width="1024" height="768" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="wall" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#F8FAFC;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E2E8F0;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow">
      <feDropShadow dx="2" dy="2" stdDeviation="2" flood-opacity="0.2"/>
    </filter>
  </defs>
  
  <!-- Room walls -->
  <rect width="1024" height="768" fill="url(#wall)"/>
  
  <!-- Floor -->
  <rect x="0" y="600" width="1024" height="168" fill="#8B4513"/>
  <rect x="0" y="600" width="1024" height="10" fill="#654321"/>
  
  <!-- Window with city view -->
  <rect x="700" y="150" width="250" height="200" fill="#87CEEB"/>
  <rect x="700" y="150" width="250" height="200" fill="none" stroke="#4A5568" stroke-width="6"/>
  <rect x="720" y="170" width="100" height="160" fill="none" stroke="#4A5568" stroke-width="3"/>
  <rect x="830" y="170" width="100" height="160" fill="none" stroke="#4A5568" stroke-width="3"/>
  
  <!-- King bed -->
  <rect x="250" y="350" width="300" height="200" fill="#4A5568" filter="url(#shadow)"/>
  <rect x="250" y="330" width="300" height="20" fill="#2D3748"/>
  <rect x="260" y="360" width="280" height="180" fill="#FFFFFF"/>
  <rect x="260" y="360" width="280" height="40" fill="#E5E7EB"/>
  
  <!-- Pillows -->
  <rect x="280" y="380" width="80" height="60" fill="#FFFFFF" stroke="#D1D5DB" stroke-width="2" filter="url(#shadow)"/>
  <rect x="440" y="380" width="80" height="60" fill="#FFFFFF" stroke="#D1D5DB" stroke-width="2" filter="url(#shadow)"/>
  
  <!-- Nightstands -->
  <rect x="100" y="450" width="120" height="100" fill="#8B4513" filter="url(#shadow)"/>
  <rect x="110" y="440" width="100" height="10" fill="#654321"/>
  
  <rect x="580" y="450" width="120" height="100" fill="#8B4513" filter="url(#shadow)"/>
  <rect x="590" y="440" width="100" height="10" fill="#654321"/>
  
  <!-- Lamps -->
  <circle cx="160" cy="420" r="25" fill="#FBBF24" filter="url(#shadow)"/>
  <rect x="150" y="420" width="20" height="30" fill="#6B7280"/>
  
  <circle cx="640" cy="420" r="25" fill="#FBBF24" filter="url(#shadow)"/>
  <rect x="630" y="420" width="20" height="30" fill="#6B7280"/>
  
  <!-- Work desk -->
  <rect x="50" y="520" width="150" height="80" fill="#8B4513" filter="url(#shadow)"/>
  <rect x="60" y="510" width="130" height="10" fill="#654321"/>
  
  <!-- Chair -->
  <rect x="80" y="560" width="60" height="40" fill="#6B7280" filter="url(#shadow)"/>
  <rect x="85" y="520" width="50" height="40" fill="#6B7280" filter="url(#shadow)"/>
  
  <!-- Room amenities -->
  <rect x="300" y="280" width="200" height="30" fill="#1F2937" rx="5"/>
  <text x="400" y="300" font-family="Arial, sans-serif" font-size="16" fill="white" text-anchor="middle">
    Eko hotel room
  </text>
  
  <!-- Realistic photo label -->
  <rect x="20" y="20" width="200" height="40" fill="#EF4444" rx="20"/>
  <text x="120" y="45" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">
    REAL PHOTO
  </text>
  
  <!-- Professional photography watermark -->
  <text x="512" y="740" font-family="Arial, sans-serif" font-size="12" fill="#6B7280" text-anchor="middle">
    Professional Hotel Photography - 4K Ultra Realistic
  </text>
</svg>